<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.or.codelabs.workshoplayout" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".RelativeLayout"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".FrameLayout"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".MenuUtama"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LinearLayout"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".AbsoluteLayout"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
